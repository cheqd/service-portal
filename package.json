{
  "name": "@cheqd/service-portal",
  "version": "1.0.0",
  "description": "Cheqd API Service Portal",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheqd/service-portal.git"
  },
  "keywords": [
	"ssi",
	"identity",
	"verifiable-credentials"
  ],
  "author": "Cheqd Foundation Limited",
  "bugs": {
    "url": "https://github.com/cheqd/service-portal/issues"
  },
  "homepage": "https://github.com/cheqd/service-portal#readme",
  "scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"files": [
		".svelte-kit/**/*",
		"LICENSE",
		"package.json",
		"README.md"
	],
	"devDependencies": {
		"@cntr/sveltekit": "^0.0.1",
		"@playwright/experimental-ct-svelte": "^1.32.3",
		"@playwright/test": "^1.32.1",
		"@rgossiaux/svelte-headlessui": "^1.0.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.15.9",
		"@tailwindcss/typography": "^0.5.9",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"autoprefixer": "^10.4.14",
		"cssnano": "^6.0.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.23",
		"prettier-plugin-svelte": "^2.10.0",
		"prettier-plugin-tailwindcss": "^0.2.7",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"tailwind-merge": "^1.12.0",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.5.0",
		"uuid": "^9.0.0",
		"vite": "^4.3.5",
		"vitest": "^0.30.1",
		"zod": "^3.21.4"
	},
	"type": "module",
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
